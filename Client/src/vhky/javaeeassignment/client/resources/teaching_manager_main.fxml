<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vhky.javaeeassignment.client.controllers.TeachingManagerMainPageController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Button mnemonicParsing="false" onAction="#onPublishBulletin" text="Publish Bulletin" />
    <Button mnemonicParsing="false" onAction="#onPrintScores" text="Print Scores">
        <VBox.margin><Insets top="10" /></VBox.margin>
    </Button>
    <Button mnemonicParsing="false" onAction="#onQuit" text="Quit">
        <VBox.margin><Insets top="10" /></VBox.margin>
    </Button>
</VBox>
